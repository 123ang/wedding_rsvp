{
  "expo": {
    "name": "JS â™¥ PS Wedding",
    "slug": "jsps-wedding",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#FF6B9D"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jspswedding.app",
      "buildNumber": "1.0.0",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "This app needs access to your photo library to upload wedding photos.",
        "NSCameraUsageDescription": "This app needs access to your camera to take wedding photos."
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#FF6B9D"
      },
      "package": "com.jspswedding.app",
      "versionCode": 1,
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
    },
    "extra": {
      "eas": {
        "projectId": "your-project-id-here"
      }
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app needs access to your photos to upload wedding memories."
        }
      ]
    ]
  }
}
