# Supabase Database Connection Settings
# Copy this file to .env and fill in your actual credentials

# Supabase PostgreSQL Database Host
# Format: db.xxxxx.supabase.co
SUPABASE_DB_HOST=db.gixbazstflmpsjksstjc.supabase.co

# Supabase Database Port (usually 5432)
SUPABASE_DB_PORT=5432

# Supabase Database Name (usually 'postgres')
SUPABASE_DB_NAME=postgres

# Supabase Database User (usually 'postgres')
SUPABASE_DB_USER=postgres

# Supabase Database Password
# Get this from: Supabase Dashboard -> Settings -> Database -> Connection string
# Look for the password in the connection string
SUPABASE_DB_PASSWORD=your_supabase_db_password_here
